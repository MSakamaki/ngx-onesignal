{
  "name": "ngx-onesignal",
  "version": "0.0.0",
  "requiredAngularVersion": ">=7.0.0",
  "description": "angular 7+ OneSignal Service",
  "author": "M.Sakamaki <mail@m.sakamaki.jp>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000",
    "build.test": "ng build -c production && npx http-server -p 4000 ./dist/ngx-onesignal",
    "build": "ng build one-singal",
    "build.s":"rm -fr dist/schematics && npx tsc -p schematics/tsconfig.json && cp schematics/collection.json dist/schematics && cp schematics/ng-add/schema.json dist/schematics/ng-add",
    "format": "prettier src/**/*.ts tools/**/*.ts projects/**/*.ts schematics/**/*.ts --write",
    "test": "ng test one-singal",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "main": "./bundles/one-singal.umd.js",
  "schematics": "./dist/schematics/collection.json",
  "keywords": [
    "typescript",
    "Angular2",
    "onesignal",
    "webpush"
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/core": "~7.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular-devkit/schematics": "~7.2.0",
    "@angular-devkit/schematics-cli": "^0.13.8",
    "@angular/animations": "~7.2.0",
    "@angular/cli": "~7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "core-js": "^2.5.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.3.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "zone.js": "~0.8.26"
  }
}
